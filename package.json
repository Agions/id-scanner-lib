{
  "name": "id-scanner-lib",
  "version": "1.5.0",
  "description": "Browser-based ID card, QR code, and face recognition scanner with liveness detection",
  "main": "dist/id-scanner-lib.js",
  "module": "dist/id-scanner-lib.esm.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist",
    "src",
    "LICENSE",
    "README.md"
  ],
  "bin": {
    "id-scanner-scaffold": "./tools/scaffold.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "rimraf dist && rollup -c rollup.config.js",
    "dev": "rollup -c -w rollup.config.js",
    "test": "jest",
    "lint": "eslint --ext .ts,.js src",
    "format": "prettier --write \"src/**/*.{ts,js}\"",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs",
    "prepare": "npm run build",
    "scaffold": "node ./tools/scaffold.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/agions/id-scanner-lib.git"
  },
  "keywords": [
    "id-scanner",
    "id-card",
    "qr-code",
    "scanner",
    "face-recognition",
    "id-verification",
    "face-detection",
    "face-liveness",
    "biometrics",
    "webcam"
  ],
  "author": "Your Name",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yourusername/id-scanner-lib/issues"
  },
  "homepage": "https://github.com/yourusername/id-scanner-lib#readme",
  "dependencies": {
    "@tensorflow/tfjs": "^4.16.0",
    "@vladmandic/face-api": "^1.7.13",
    "jsqr": "^1.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@babel/preset-env": "^7.23.7",
    "@babel/preset-typescript": "^7.23.7",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/jest": "^29.5.11",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "dts-bundle-generator": "^9.2.4",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "prettier": "^3.2.4",
    "rimraf": "^5.0.5",
    "rollup": "^4.9.6",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-polyfill-node": "^0.13.0",
    "rollup-plugin-serve": "^1.1.1",
    "rollup-plugin-typescript2": "^0.36.0",
    "ts-jest": "^29.1.1",
    "tslib": "^2.6.2",
    "typedoc": "^0.25.7",
    "typescript": "^5.3.3",
    "vitepress": "^1.0.0-rc.25"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
